putchar := proc(char: s32) -> s32 #extern "putchar"

array: [cast(u8) (cast(s8) 10 * 10)]u8

{
    i := 0
    char: u8 = 65
    while char != 91 {
        array[i] = char
        i = i + 1
        array[i] = char + 32
        i = i + 1
        array[i] = 32
        i = i + 1

        char = char + 1
    }
    array[i] = 10
    i = i + 1
    array[i] = 0
    i = i + 1
}

i := 0
while array[i] != 0 {
    putchar(cast(s32) array[i])
    i = i + 1
}
