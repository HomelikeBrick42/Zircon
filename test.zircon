putchar := proc(char: s32) -> s32 #extern "putchar"

array: [2 * 3 + 1]u8
array_length := size_of(type_of(array)) / size_of(type_of(array[0]))

{
    i := 0
    while i != array_length - 1 {
        array[i] = 69
        i = i + 1
    }
    array[i] = 0
}

{
    i := 0
    while array[i] != 0 {
        putchar(cast(s32) array[i])
        i = i + 1
    }
}
